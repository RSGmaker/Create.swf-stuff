		<custom id="Forest Path(Night)" author="ShinKong" description="Obtained from: http://fav.me/d9bdr22" scales="1,2"/>
		<custom id="Forest Top View" author="ShinKong" description="Obtained from: http://fav.me/d9bdr22" scales="1,2"/>
		<custom id="Hakugyokurou Entrance Path" author="ShinKong" description="Obtained from: http://fav.me/d9bdr22" scales="1,2"/>
